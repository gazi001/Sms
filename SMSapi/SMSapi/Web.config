<?xml version="1.0"?>
<configuration>
  <configSections>
  </configSections>
  <appSettings/>
  <connectionStrings/>
  <!--
    有关 .NET 4.5 的 web.config 更改的说明，请参见 http://go.microsoft.com/fwlink/?LinkId=235367。

    可在 <httpRuntime> 标记上设置以下特性。
      <system.Web>
        <httpRuntime targetFramework="4.5" />
      </system.Web>
  -->
  <system.web>
    <compilation debug="true" targetFramework="4.5"/>
    <!--
            通过 <authentication> 节，可配置 
      ASP.NET 用于识别进入用户的 
      安全身份验证模式。
    -->
    <authentication mode="Windows"/>
    <!--
            通过 <customErrors> 节，可以配置
       在执行请求的过程中出现未处理的错误时要执行 
       的操作。具体而言，
       开发人员通过该节可配置要显示的 html 错误页，
       以代替错误堆栈跟踪。

        <customErrors mode="RemoteOnly" defaultRedirect="GenericErrorPage.htm">
            <error statusCode="403" redirect="NoAccess.htm" />
            <error statusCode="404" redirect="FileNotFound.htm" />
        </customErrors>
        -->
    <pages controlRenderingCompatibilityVersion="3.5" clientIDMode="AutoID"/>
  </system.web>
  <!-- 
        system.webServer 节是在 Internet Information Services 7.0 下运行 ASP.NET AJAX 
        所必需的。对于早期版本的 IIS，不必保留此节。
  -->
  <system.serviceModel>
    <bindings>
      <basicHttpBinding>
        <binding name="SmsPortSoap" />
      </basicHttpBinding>
      <customBinding>
        <binding name="SmsPortSoap12">
          <textMessageEncoding messageVersion="Soap12" />
          <httpTransport />
        </binding>
      </customBinding>
    </bindings>
    <client>
      <endpoint address="http://61.191.26.189:8888/SmsPort.asmx" binding="basicHttpBinding"
        bindingConfiguration="SmsPortSoap" contract="SmsPort.SmsPortSoap"
        name="SmsPortSoap" />
      <endpoint address="http://61.191.26.189:8888/SmsPort.asmx" binding="customBinding"
        bindingConfiguration="SmsPortSoap12" contract="SmsPort.SmsPortSoap"
        name="SmsPortSoap12" />
    </client>
  </system.serviceModel>
</configuration>